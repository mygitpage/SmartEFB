<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:gravity="left"
    android:paddingBottom="5dp"
    android:paddingTop="5dp"
    android:paddingLeft="10dp"
    android:paddingRight="10dp">

    <TextView
        android:id="@+id/suggestionFromClientBorderBetween"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/text_color"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="20dp"
        android:layout_marginTop="20dp"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/suggestionFromClientIntroInfoText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18dp"
        android:textColor="@color/colorPrimaryDark"
        android:gravity="left"
        android:paddingLeft="10dp"
        android:paddingBottom="20dp"
        android:textStyle="bold"
        android:visibility="gone"/>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/list_element_style">

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/suggestionFromClientTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:textColor="@color/colorPrimaryDark"
                android:gravity="left"
                android:textStyle="bold"
                android:layout_marginLeft="3dp"/>

            <TextView
                android:id="@+id/suggestionFromClientNewEntryText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14dp"
                android:textColor="@color/text_accent_color"
                android:gravity="left"
                android:paddingLeft="10dp"/>

        </LinearLayout>

        <TextView
            android:id="@+id/suggestionFromClientAuthorAndDate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="13dp"
            android:textColor="@color/colorPrimaryDark"
            android:gravity="left"
            android:layout_marginLeft="3dp"/>

    </LinearLayout>

    <!-- info post out off timezone -->
    <TextView
        android:id="@+id/suggestionFromClientPostOutOffTimezone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:textColor="@color/text_accent_color"
        android:gravity="center"
        android:paddingTop="15dp"
        android:visibility="gone"
        android:text="@string/suggestionFromClientPostOutOffTimezoneText"/>
    <!-- end info post out off timezone -->

    <!-- info canceled suggestion from client -->
    <TextView
        android:id="@+id/suggestionFromClientCanceledInvitationText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:textColor="@color/text_accent_color"
        android:gravity="center"
        android:paddingTop="15dp"
        android:visibility="gone"
        android:text="@string/suggestionFromClientTimezoneCanceledByCoach"/>
    <!-- end info canceled suggestion from client -->

    <!-- info meeting found from client suggestion -->
    <TextView
        android:id="@+id/suggestionFromClientMeetingFoundText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:textColor="@color/text_accent_color"
        android:gravity="center"
        android:paddingTop="15dp"
        android:visibility="gone"
        android:text="@string/suggestionFromClientMeetingFoundByCoachText"/>
    <!-- end info meeting found from client suggestion -->


    <TextView
        android:id="@+id/errorInputSuggestionFromClient"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="14dp"
        android:textColor="@color/text_accent_color"
        android:gravity="left"
        android:visibility="gone"
        android:text="@string/errorInputClientSuggestion"
        android:paddingBottom="5dp"
        android:paddingTop="15dp"/>

    <TextView
        android:id="@+id/countLettersSuggestionFromClientEditText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="13dp"
        android:textColor="@color/colorPrimaryDark"
        android:gravity="right"
        android:visibility="gone"
        android:paddingTop="15dp"/>

    <EditText
        android:id="@+id/inputSuggestionFromClientText"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:textColor="@color/colorPrimaryDark"
        android:gravity="left|top"
        android:background="@drawable/app_edittext_style"
        android:hint="@string/suggestionFromClientHintText"
        android:layout_margin="5dp"
        android:textColorHint="@color/colorPrimaryDark"
        android:inputType="textNoSuggestions|text|textMultiLine|textFilter"
        android:padding="5dp"
        android:digits="@string/editTextInputFilterTextAlphaNumeric"
        android:visibility="gone"/>


    <!-- show input suggestion from client -->
    <TextView
        android:id="@+id/suggestionFromClientInputSuggestionText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        android:textColor="@color/colorPrimaryDark"
        android:gravity="left"
        android:layout_marginTop="20dp"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/suggestionFromClientInputSuggestionHintText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="14dp"
        android:textColor="@color/text_accent_color"
        android:paddingTop="15dp"
        android:gravity="center"
        android:text="@string/suggestionFromClientAllreadySendInputSuggestionHintText"
        android:visibility="gone"/>

    <!-- end show input suggestion from client -->



    <!-- coach hint for client suggestion -->
    <TextView
        android:id="@+id/suggestionFromClientCoachHintText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        android:textColor="@color/colorPrimaryDark"
        android:gravity="left"
        android:layout_marginTop="20dp"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/suggestionFromClientCoachHintTextBorder"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/text_color"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="20dp"
        android:layout_marginTop="20dp"
        android:visibility="gone"/>

    <!-- end coach hint for client suggestion -->


    <!-- timer for response block -->
    <TextView
        android:id="@+id/suggestionFromClientEndDateTicTimerIntroText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        android:textColor="@color/colorPrimaryDark"
        android:gravity="center"
        android:text="@string/suggestionFromClientEndDateTimeAskedForIntroText"
        android:layout_marginTop="20dp"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/suggestionFromClientEndDateTicTimer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18dp"
        android:textColor="@color/text_accent_color"
        android:gravity="center"
        android:textStyle="bold"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/suggestionFromClientEndDateTicTimerInText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        android:textColor="@color/colorPrimaryDark"
        android:gravity="center"
        android:visibility="gone"/>
    <!-- end timer for response block -->


    <Button
        android:id="@+id/buttonSendSuggestionFromClientToCoach"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/app_button_style"
        android:textColor="@color/text_color_white"
        android:layout_gravity="center_horizontal"
        android:text="@string/suggestionFromClientButtonSendToCoach"
        android:padding="10dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:visibility="gone"/>


    <TextView
        android:id="@+id/suggestionFromClientAtLeastInfoText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="14dp"
        android:textColor="@color/text_accent_color"
        android:gravity="center"
        android:paddingTop="15dp"
        android:visibility="gone"
        android:text="@string/suggestionFromClientInputInfoTextForUser"/>

    <TextView
        android:id="@+id/suggestionFromClientDeleteEntryLink"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="14dp"
        android:textColor="@color/text_color"
        android:textColorLink="@color/colorPrimaryDark"
        android:gravity="right"
        android:paddingTop="15dp"/>

    <TextView
        android:id="@+id/suggestionFromClientDeleteRegularyInfoText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="12dp"
        android:textColor="@color/colorPrimaryDark"
        android:gravity="right"
        android:visibility="gone"
        android:paddingBottom="15dp"/>










</LinearLayout>