<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="left"
    android:background="@color/body_background"
    android:paddingBottom="5dp"
    android:paddingTop="5dp"
    android:paddingLeft="10dp"
    android:paddingRight="10dp">

    <TextView
        android:id="@+id/textViewDialogConnectBookNoMoreMessages"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="14dp"
        android:textColor="@color/text_color"
        android:gravity="left"
        android:textStyle="bold"
        android:text="@string/textDialogConnectBookNoMoreMessages"
        android:paddingTop="5dp"
        android:paddingBottom="10dp"/>

    <TextView
        android:id="@+id/textViewDialogConnectBookInfoCoach"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="14dp"
        android:textColor="@color/text_color"
        android:gravity="left"
        android:textStyle="bold"
        android:text="@string/textDialogConnectBookInfoCoach"
        android:paddingTop="5dp"
        android:paddingBottom="10dp"/>


</LinearLayout>